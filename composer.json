{
    "name": "ghostzero/broom",
    "description": "Cleanup tool for vendor and node_modules directories.",
    "keywords": [
        "broom",
        "composer",
        "node_modules",
        "cleanup"
    ],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "René Preuß",
            "email": "rene@preuss.io"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "laravel-zero/framework": "^8.4"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.2",
        "pestphp/pest": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "broom"
    ]
}
